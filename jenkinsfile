#!/usr/bin/env groovy

@Library('my-jenkins-shared') _     
def tools = new com.aladingziben.devops.devops()


pipeline {
    agent { node {  label "master" }}

    stages {
        //下载代码
        stage("GetCode"){ 
            steps{  
                timeout(time:5, unit:"MINUTES"){   
                    script{ 
                        tools.PrintMes("获取代码",'green')
                        hello.call()
                    }
                }
            }
        }
    }
}
